    "def getLinkSoup(link):\n",
    "    response = requests.get(\n",
    "        url=link,\n",
    "    )\n",
    "    if response.status_code != 200:\n",
    "        print(f\"> ERROR: Link [{link}] not found...\")\n",
    "        return None\n",
    "    soup = BeautifulSoup(response.content, 'html.parser')\n",
    "    return soup\n"
   ]
